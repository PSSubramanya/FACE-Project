<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:height="230dp"
        android:width="230dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <solid android:color="#FFFFFF"></solid>
            <corners android:topRightRadius="240dp"
                android:bottomRightRadius="240dp"
                android:bottomLeftRadius="240dp"
                android:topLeftRadius="240dp"/>
            <stroke
                android:width="1dp"
                android:color="@android:color/darker_gray" />
        </shape>
    </item>
    <item android:height="160dp"
        android:width="160dp"
        android:gravity="center"
        android:drawable="@drawable/musicbackground">
    </item>
</layer-list>

